{
	"name": "dynamics-crm-toolkit",
	"displayName": "dynamics-crm-toolkit",
	"description": "A toolkit for working with Dynamics CRM within VSCode by CloudSmith Consulting LLC.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dynamicsConnections",
		"onCommand:extension.cloudSmith",
		"onCommand:extension.tellTime"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.cloudSmith",
				"title": "CloudSmith: CloudSmith VSCode"
			},
			{
				"command": "extension.tellTime",
				"title": "CloudSmith: Tell Time"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dynamics-explorer",
					"title": "CloudSmith",
					"icon": "media/dynamics2.png"
				}
			]
		},
		"views": {
			"dynamics-explorer": [
				{
					"id": "dynamicsConnections",
					"name": "Dynamics Connections"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}