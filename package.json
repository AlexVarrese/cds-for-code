{
	"name": "dynamics-365-toolkit",
	"displayName": "dynamics-365-toolkit",
	"description": "A toolkit for working with Dynamics 365 within VSCode by CloudSmith Consulting LLC.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dynamicsConnectionsView",
		"onCommand:cloudSmith.unpackDynamicsSolutionCommand",
		"onCommand:cloudSmith.deployDynamicsSolutionCommand",
		"onCommand:cloudSmith.generateDynamicsEntitiesCommand"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Dynamics365",
			"properties": {
				"cloudSmith.crmSvcUtilPath": {
					"type": "string",
					"default": "C:\\Deploy\\Tools\\CoreTools",
					"description": "The absolute path of the CrmSvcUtil.exe file installed by the CRM SDK"
				}
			}
		},
		"commands": [
			{
				"command": "cloudSmith.unpackDynamicsSolutionCommand",
				"title": "CloudSmith: Unpack Solution From Dynamics Server"
			},
			{
				"command": "cloudSmith.deployDynamicsSolutionCommand",
				"title": "CloudSmith: Deploy Solution to Dynamics Server"
			},
			{
				"command": "cloudSmith.generateDynamicsEntitiesCommand",
				"title": "CloudSmith: Generate Entities From Dynamics Server"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "view-container",
					"title": "CloudSmith",
					"icon": "media/dynamics.png"
				}
			]
		},
		"views": {
			"view-container": [
				{
					"id": "dynamicsConnectionsView",
					"name": "Dynamics Connections"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {}
}
